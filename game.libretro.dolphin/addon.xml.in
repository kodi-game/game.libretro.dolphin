<?xml version="1.0" encoding="UTF-8"?>
<addon id="game.libretro.dolphin"
		name="Nintendo - GameCube / Wii (Dolphin)"
		version="89.0.0.27"
		provider-name="Team Dolphin">
	<requires>
		<import addon="game.libretro" version="1.0.0"/>
		<import addon="game.controller.nes" version="1.0.0"/>
	</requires>
	<extension point="kodi.gameclient"
			library_@PLATFORM@="@LIBRARY_FILENAME@">
		<platforms>NGC</platforms>
		<extensions>elf|dol|gcm|iso|tgc|wbfs|ciso|gcz|wad|wia|rvz|m3u</extensions>
		<supports_vfs>false</supports_vfs>
		<supports_standalone>false</supports_standalone>
		<requires_opengl>true</requires_opengl>
	</extension>
	<extension point="xbmc.addon.metadata">
		<license>GPLv2+</license>
		<platform>@PLATFORM@</platform>
		<source>https://github.com/kodi-game/game.libretro.dolphin</source>
		<assets>
			<icon>resources/icon.png</icon>
		</assets>
		<summary lang="da_DK">Nintendo - GameCube / Wii (Dolphin)</summary>
		<summary lang="en_GB">Nintendo - GameCube / Wii (Dolphin)</summary>
		<summary lang="es_ES">Nintendo - GameCube / Wii (Dolphin)</summary>
		<summary lang="pt_BR">Nintendo - GameCube / Wii (Dolphin)</summary>
		<summary lang="sv_SE">Nintendo - GameCube / Wii (Dolphin)</summary>
		<summary lang="zh_CN">任天堂 - GameCube / Wii (Dolphin)</summary>
		<description lang="da_DK">Dolphin er en emulator til to nyere Nintendo-videospilkonsoller: GameCube og Wii.</description>
		<description lang="de_DE">Dolphin ist ein Emulator für zwei aktuelle Nintendo-Spielkonsolen: den GameCube und die Wii.</description>
		<description lang="en_GB">Dolphin is an emulator for two recent Nintendo video game consoles: the GameCube and the Wii.</description>
		<description lang="es_ES">Dolphin es un emulador para dos videoconsolas recientes de Nintendo: la GameCube y la Wii.</description>
		<description lang="fi_FI">Dolphin on emulaattori Nintendon GameCube- ja Wii-konsoleille.</description>
		<description lang="pt_BR">O Dolphin é um emulador para dois consoles de videogame recentes da Nintendo: o GameCube e o Wii.</description>
